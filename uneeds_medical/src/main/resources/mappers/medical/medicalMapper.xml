<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
	PUBLIC "-//mybatis.org//DTD Config 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.medical.mappers.medicalMapper">	<!-- namespace = package / 임의 설정 -->
	<select id="getTime" resultType="string">
		select now()
	</select>
	
	<!-- 병원예약 정보 가져오기 -->
	<select id="medicalList" resultType="string">
		select * from m_reservation;
	</select>
	
	<!-- 병원 정보 입력하기 -->
	
	<!-- sido code insert -->
	<insert id="sidoInsert">
	 insert into m_sido values(#{sicode}, #{siname})
	</insert>
	
	<!-- sigun code insert -->
	<insert id="sigunInsert">
	 insert into m_sigun values(#{guncode}, #{gunname})
	</insert>

	<!-- 진료과목 insert -->
	<insert id="treatmentInsert">
	 insert into m_treatment values(#{mtcode}, #{mtname})
	</insert>
	
	<!-- 의료과목 insert -->
	<insert id="kindsInsert">
	 insert into m_kinds values(#{mkcode}, #{mkname})
	</insert>
		
	<select id="selectSido" resultType="com.medical.domain.SidoVO">
	select * from m_sido	
	</select>
	
	
</mapper>