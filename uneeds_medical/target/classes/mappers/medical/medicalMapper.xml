<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
	PUBLIC "-//mybatis.org//DTD Config 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.medical.mappers.medicalMapper">	<!-- namespace = package / 임의 설정 -->
	<select id="getTime" resultType="string">
		select now()
	</select>
	
	

	
	<!-- Insert -->
	<!-- sido code insert -->
	<insert id="insertSido">
	 insert into m_sido values(#{sicode}, #{siname})
	</insert>
	
	<!-- sigun code insert -->
	<insert id="insertSigun">
	 insert into m_sigun values(#{guncode}, #{gunname})
	</insert>
	
	<!-- 진료과목 insert -->
	<insert id="insertThema">
	 insert into m_treatment values(#{mtnumber}, #{mtname})
	</insert>
	
	<!-- 의료과목 insert -->
	<insert id="insertKinds">
	 insert into m_kinds values(#{mknumber}, #{mkname})
	</insert>
	

	
</mapper>